<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
  <servlet-name>Login</servlet-name>
  <servlet-class>main.com.servlets.logIn</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>insertDB</servlet-name>
    <servlet-class>main.com.servlets.insertDB</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>View_Table</servlet-name>
    <servlet-class>main.com.servlets.ReturnFromDB</servlet-class>
  </servlet>
  
  
  <servlet-mapping>
    <servlet-name>Login</servlet-name>
    <url-pattern>/LogIn</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>insertDB</servlet-name>
    <url-pattern>/InsertToDatabase</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>View_Table</servlet-name>
    <url-pattern>/ReturnTable</url-pattern>
  </servlet-mapping>

</web-app>
